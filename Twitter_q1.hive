select get_json_object(rawtweets.data,"$.user.screen_name") as scr_name, from_unixtime(unix_timestamp(get_json_object(rawtweets.data,"$.created_at"), "EEE MMM dd hh:mm:ss Z yyyy"), 'hh') as hh , count(*) from rawtweets  where  get_json_object(rawtweets.data,"$.user.screen_name") == 'PrezOno' group by from_unixtime(unix_timestamp(get_json_object(rawtweets.data,"$.created_at"), "EEE MMM dd hh:mm:ss Z yyyy"), 'hh'), get_json_object(rawtweets.data,"$.user.screen_name")  ;
